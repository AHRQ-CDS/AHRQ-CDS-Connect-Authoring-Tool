{
  "name": "cds_authoring_tool",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.15.3",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "foreman": "^2.0.0",
    "history": "^4.6.1",
    "immutability-helper": "^2.1.2",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "moment": "^2.18.1",
    "mongoose": "^4.9.1",
    "nodemon": "^1.11.0",
    "react": "^15.4.2",
    "react-axe": "^2.1.1",
    "react-dnd": "^2.3.0",
    "react-dnd-html5-backend": "^2.3.0",
    "react-dom": "^15.4.2",
    "react-fontawesome": "^1.5.0",
    "react-input-autosize": "1.1.0",
    "react-modal": "^1.7.7",
    "react-router-dom": "^4.0.0",
    "react-select": "1.0.0-rc.3",
    "slug": "^0.9.1"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.8.1",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-react-app": "^0.6.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "node-bourbon": "^4.2.8",
    "node-sass": "^4.5.1",
    "react-addons-test-utils": "^15.5.1",
    "react-dnd-test-backend": "^2.3.0",
    "react-router-test-context": "^0.1.0",
    "react-scripts": "0.9.5"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./node_modules/bourbon/app/assets/stylesheets --include-path ./node_modules/font-awesome/scss src/ -o src/",
    "watch-css": "npm run build-css && node-sass --include-path ./node_modules/bourbon/app/assets/stylesheets --include-path ./node_modules/font-awesome/scss src/ -o src/ --watch --recursive",
    "start": "npm run watch-css && react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom --collectCoverageFrom='[\"*.js\", \"model/*.js\", \"src/**/*.{js,jsx}\", \"!src/index.js\", \"!setupTests.js\"]' --noStackTrace",
    "eject": "react-scripts eject",
    "start-dev": "nf start -p 3000",
    "lint": "eslint --fix src/*; exit 0"
  }
}
